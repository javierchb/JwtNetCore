@{
}
@model WebMVC.UIModels.UIUser

<div class="row justify-content-center">
    <div class="col-12 col-sm-3 col-md-3 col-lg-3">
        <div class="card">
            <div class="card-header">Employees list</div>
            <div class="card-body">
                <ul class="list-group">
                    @foreach (var item in Model.ViewEmployees.Employees)
                    {
                        <li class="list-group-item">@item.EmployeeName</li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>